<Application x:Class="EroMangaManager.UWP.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
             xmlns:converter="using:EroMangaManager.UWP.ValueConverters">
    <Application.Resources>
        <muxc:XamlControlsResources>
            <muxc:XamlControlsResources.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionarys/DefaultTheme.xaml" />
                <ResourceDictionary Source="ResourceDictionarys/TitleContentPairTheme.xaml" />
                <ResourceDictionary Source="ResourceDictionarys/ValueConverters.xaml" />
            </muxc:XamlControlsResources.MergedDictionaries>
        </muxc:XamlControlsResources>
    </Application.Resources>
</Application>