<Application x:Class="EroMangaManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:muxc="using:Microsoft.UI.Xaml.Controls"
             xmlns:converter="using:EroMangaManager.ValueConverters">
    <Application.Resources>
        <muxc:XamlControlsResources>
            <muxc:XamlControlsResources.MergedDictionaries>
                <ResourceDictionary Source="Themes/DefaultTheme.xaml" />
                <ResourceDictionary Source="Themes/TitleContentPairTheme.xaml" />
                <ResourceDictionary Source="Themes/Dictionary1.xaml" />
            </muxc:XamlControlsResources.MergedDictionaries>
        </muxc:XamlControlsResources>
    </Application.Resources>
</Application>