<Page
    x:Class="EroMangaManager.Views.UpdateRecordsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EroMangaManager.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Themes/UpdateRecordPageTheme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <StackPanel  Orientation="Vertical">
        <TextBlock x:Uid="/UpdateResources/Version_2022.12.26" Style="{StaticResource UpdateVersion}" />
        <TextBlock x:Uid="/UpdateResources/Description_2022.12.26" Style="{StaticResource UpdateDescription}"/>

        <TextBlock x:Uid="/UpdateResources/Version_2022.12.19"
                   Style="{StaticResource UpdateVersion}" />
        <TextBlock x:Uid="/UpdateResources/Description_2022.12.19"
                   Style="{StaticResource UpdateDescription}" />

    </StackPanel>
</Page>
