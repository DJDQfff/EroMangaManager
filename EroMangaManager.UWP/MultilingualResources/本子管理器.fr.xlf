  <file datatype="xml" source-language="zh-CN" target-language="fr" original="本子管理器/STRINGS/ZH-CN/USAGEDOCUMENT.RESW" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
